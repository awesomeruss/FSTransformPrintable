              "5851a861bce12": {
                "Editable": true,
                "ID": "5851a861bce12",
                "Name": "GlobSec - List Office Locations",
                "Type": "DATABASE",
                "LIM_name": "Local LIM",
                "Category": "GlobSec",
                "data_type": "lookup",
                "allow_offline": 0,
                "Output_template": "{\"lookup_validation\":\"0\",\"systemID\":\"5aec5ae6942e2\",\"systemName\":\"FirmstepCoreGlobalSecurity\",\"query\":\"SELECT TOP (1000) ol.[id] as name\\n      ,[companyName]\\n      ,[longCompanyName]\\n      ,[displayCompanyName] as display\\n      ,[utcOffset]\\n      ,[addressLine1]\\n      ,[addressLine2]\\n      ,[townCity]\\n      ,[region]\\n      ,[countryDisplay]\\n      ,[countryID]\\n      ,[postCode]\\n      ,[phoneNumber]\\n      ,[CASReporting]\\n      ,[LIM]\\n      ,[lat]\\n      ,[lng]\\n      ,ol.[archived]\\n\\t\\t,a.group_id as admin_group\\n\\t\\t,m.group_id as manager_group\\n\\t\\t,d.group_id as director_group\\n  FROM [FirmstepLookupsDev].[dbo].[OfficeLocation] ol\\n\\tleft join (OfficeLocationRole a inner join role ar on a.role_id=ar.id) on ol.id=a.office_location_id and ar.name = 'ERROL Admin'\\n\\tleft join (OfficeLocationRole m inner join role mr on m.role_id=mr.id) on ol.id=m.office_location_id and mr.name = 'ERROL Manager'\\n\\tleft join (OfficeLocationRole d inner join role dr on d.role_id=dr.id) on ol.id=d.office_location_id and dr.name = 'ERROL Director'\\n\\twhere ol.archived=0\\n\\torder by displayCompanyName\",\"results_page_columns_db\":\"\",\"secure_fields_db_database\":\"\",\"escapeTokens\":\"yes\"}",
                "log_only_on_error": 0,
                "error_notification": "{\"LIM_name\":\"LIM14\",\"recipients_to\":\"fs-support@awesomeconsulting.co.uk\"}"
              }
            }
